{
	"name": "intro-touch",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "sst dev",
		"build": "sst build",
		"deploy": "sst deploy",
		"remove": "sst remove",
		"console": "sst console",
		"typecheck": "tsc --noEmit",
		"gen": "hygen",
